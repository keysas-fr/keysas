#include <tunables/global>

# vim:syntax=apparmor
# AppArmor policy for keysas-in
# ###AUTHOR###
# Stephane N
# ###COPYRIGHT###
# Keysas


/usr/bin/keysas-in {
  #include <abstractions/apache2-common>
  #include <abstractions/base>

  /sys/devices/system/cpu/online r,
  owner /run/keysas/ r,
  owner /run/keysas/socket_in rw,
  /var/local/in/ r,
  /var/local/in/** rw,
}
